</fieldset>	

<!-- style settings -->
<fieldset name="style_settings" label="STYLE_SETTINGS">	
	<field name="bgim_color" type="list" default="show" label="BGIMG_SHOW" description="" override="true">	
	<option value="show">Show</option>
	<option value="hide">Hide</option>
	</field>
	
	<field name="body_bgim_color" type="media"  default="" label="BGIM_COLOR" description="COLORIM_DESC" override="true" />		
<field type="spacer" label="ONLY_IN_PRO" />			
	<field name="predefined_style" type="list" default="red" label="STYLE" description="" disabled="true">	
	<option value="blue" disabled="true">BLUE</option>
	<option value="green" disabled="true">GREEN</option>
	<option value="orange" disabled="true">ORANGE</option>
	<option value="red" disabled="true">RED</option>
	</field>
	
	<field type="spacer" />
	<field type="spacer" label="CUSTOM_STYLE_MORE" />	
	
	<field name="color_custom_style" type="text" default="" label="CUSTOM_STYLE" description="COLOR_DESC" disabled="true" class="color {pickerPosition:'right',hash:true,caps:false,required:false}" />
	<field name="color_text" type="text" default="" label="COLOR_TEXT" description="COLOR_DESC" disabled="true" class="color {pickerPosition:'right',hash:true,caps:false,required:false}" />
	<field name="color_links" type="text" default="" label="COLOR_LINKS" description="COLOR_DESC" disabled="true" class="color {pickerPosition:'right',hash:true,caps:false,required:false}" />	
	<field name="color_links_hover" type="text" default="" label="COLOR_LINKS_HOVER" description="COLOR_DESC" disabled="true" class="color {pickerPosition:'right',hash:true,caps:false,required:false}" />
	<field name="body_bg_color" type="text" default="" label="BG_COLOR" description="COLOR_DESC" disabled="true" class="color {pickerPosition:'right',hash:true,caps:false,required:false}" />
	
				
</fieldset>

