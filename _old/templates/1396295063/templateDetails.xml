	<files>
		<folder>admin</folder>
		<folder>html</folder>
		<folder>css</folder>
		<folder>framework</folder>
		<folder>images</folder>
		<folder>language</folder>
		<folder>header</folder>
		<folder>scripts</folder>
		<filename>index.php</filename>
		<filename>index.html</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<filename>component.php</filename>
		<filename>error.php</filename>
	</files>

	<positions>
		<position>breadcrumb</position>
		<position>advert1</position>
		<position>advert2</position>
		<position>advert3</position>
		<position>advert4</position>
		<position>top1</position>
		<position>top2</position>
		<position>top3</position>
		<position>top4</position>
		<position>info1</position>
		<position>info2</position>
		<position>info3</position>
		<position>info4</position>
		<position>bottom1</position>
		<position>bottom2</position>
		<position>bottom3</position>
		<position>bottom4</position>
		<position>mainmenu</position>
		<position>footer-menu</position>
		<position>search</position>
		<position>left</position>
		<position>right</position>
		<position>header</position>
	</positions>
		
	<languages folder="language">
		<language tag="en-GB">en-GB.tpl_ol_anteez.ini</language>
	</languages>
	
<config>

<!-- START TEMPLATE PARAMETERS -->
<fields name="params">

<fieldset name="logo_settings" label="LOGO_SETTINGS">	

	<field name="logo_type" type="list" default="text" label="LOGO_TYPE" description="" override="false">	
	<option value="text">TEXT</option>
	<option value="image">IMAGE</option>
	</field>
	<field name="logo_text" type="text" default="ANTEEZ" label="LOGO_TEXT" description="" override="false" />
	<field name="site_slogan" type="text" default="" label="SITE_SLOGAN" description="" override="false" />	
	<field name="logo_image" type="media" default="" label="LOGO_IMAGE" description="MEDIA_DESC" override="false" />
	<field name="logo_image_width" type="text" default="160px" label="LOGO_IMAGE_WIDTH" description="" filter="string" override="false" />
	<field name="logo_image_height" type="text" default="58px" label="LOGO_IMAGE_HEIGHT" description="" filter="string" override="false" />
				
</fieldset>

<fieldset name="header"  description="" label="HEADER_SETTINGS">
	<field name="slides" type="list" default="1" label="Slideshow" description="To show the Slideshow on specific pages, select [Show only in header]. After go in Extensions - Module manager and create an empty module and assign it to position header. Use this module to control which pages have header visible">
		<option value="1">Show on all pages</option>
		<option value="2">Show only in header</option>
		<option value="0">Hide</option>
	</field>		
	<field name="spacer" type="spacer" class="text" label="First Slide"/>		
	<field name="sliders_thumb1" type="media" default="" label="Slider Image 1" description="Please choose an image..." />
	<field name="sliders_texts1" type="text" default="" label="Text for first slide (pro)" description="Write text info for first slide"  disabled="true"/>
	<field name="sliders_text1" type="text" default="" label="Read more link (pro)" description="Link for read more button"  disabled="true"/>
	<field name="spacer1" type="spacer" class="text" label="Second Slide"/>	
	<field name="sliders_thumb2" type="media" default="" label="Slider Image 2" description="Please choose an image..." />
	<field name="sliders_texts2" type="text" default="" label="Text for second slide (pro)" description="Write text info for second slide"  disabled="true"/>
	<field name="sliders_text2" type="text" default="" label="Read more link (pro)" description="Link for read more button"  disabled="true"/>
	<field name="spacer2" type="spacer" class="text" label="Third Slide"/>
	<field name="sliders_thumb3" type="media" default="" label="Slider Image 3" description="Please choose an image..." />
	<field name="sliders_texts3" type="text" default="" label="Text for third slide (pro)" description="Write text info for third slide"  disabled="true"/>
	<field name="sliders_text3" type="text" default="" label="Read more link (pro)" description="Link for read more button"  disabled="true"/>
	<field name="spacer3" type="spacer" class="text" label="Fourth Slide"/>
	<field name="sliders_thumb4" type="media" default="" label="Slider Image 4" description="Please choose an image..." />
	<field name="sliders_texts4" type="text" default="" label="Text for fourth slide (pro)" description="Write text info for fourth slide"  disabled="true"/>
	<field name="sliders_text4" type="text" default="" label="Read more link (pro)" description="Link for read more button" disabled="true" />
	<field name="spacer4" type="spacer" class="text" label="Fifth Slide"/>
	<field name="sliders_thumb5" type="media" default="" label="Slider Image 5" description="Please choose an image..." />
	<field name="sliders_texts5" type="text" default="" label="Text for slide 5 (pro)" description="Write text info for slide 5"  disabled="true"/>
	<field name="sliders_text5" type="text" default="" label="Read more link (pro)" description="Link for read more button" disabled="true" />
	<field name="spacer5" type="spacer" class="text" label="Header settigns"/>
<field type="spacer" label="ONLY_IN_PRO" />		
	<field name="headHeigh" type="text" default="400" label="Header Height" description="Put a size for the header height, depend from the height of images." disabled="true" />	
	<field name="menu" type="list" default="true" label="Show Navigation" description="Show Navigation." disabled="true">
		<option value="true">Yes</option>
		<option value="false">No</option>
	</field>			    
				
	<field name="speed" type="list" default="5000" label="Speed of Annimation" description="This is the time at which the annimation happens." disabled="true">
		<option value="3000">3 Seconds</option>
		<option value="4000">4 Seconds</option>
		<option value="5000">5 Seconds</option>
		<option value="6000">6 Seconds</option>
		<option value="7000">7 Seconds</option>
		<option value="8000">8 Seconds</option>
		<option value="9000">9 Seconds</option>
		<option value="10000">10 Seconds</option>
