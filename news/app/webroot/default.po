msgid ""
"The Newsletter Mailer uses the <a href=\"http://www.smarty.net/\">SMARTY</a> template engine.<br />\n"
"\n"
"      Insert field: {$<b>fieldname</b>}<br />\n"
"      Start repeating block: <?php echo htmlspecialchars(\"<!---\");?>foreach name=<b>groupname</b> item=<b>anyname</b> from=<b>groupname</b><?php echo htmlspecialchars(\"--->\");?><br />\n"
"      Insert field in block: {$<b>anyname</b>.<b>fieldname</b>}<br />\n"
"      End repeating block:  <?php echo htmlspecialchars(\"<!---\");?>/foreach<?php echo htmlspecialchars(\"--->\");?><br />\n"
"      Conditons:<br />\n"
"      -Not empty:  <?php echo htmlspecialchars(\"<!---\");?>if !empty($<b>fieldname</b>)<?php echo htmlspecialchars(\"--->\");?><br />\n"
"      End conditon block:  <?php echo htmlspecialchars(\"<!---\");?>/if<?php echo htmlspecialchars(\"--->\");?><br />\n"
"      Image: [image src={$<b>fieldname</b>} w=<b>width</b> h=<b>height</b> border=<b>border</b> bcolor=<b>bordercolor</b> ]"
msgstr ""
"Magic Letter maakt gebruik van <a href=\"http://www.smarty.net/\">SMARTY</a> sjabloonmaker.<br />\n"
"\n"
"     Veld Toeovegen: {$<b>fieldname</b>}<br />\n"
"      Begin te herhalen blok: <?php echo htmlspecialchars(\"<!---\");?>foreach name=<b>groupname</b> item=<b>anyname</b> from=<b>groupname</b><?php echo htmlspecialchars(\"--->\");?><br />\n"
"      Veld toevoegen in blok: {$<b>anyname</b>.<b>fieldname</b>}<br />\n"
"      Einde te herhalen blok:  <?php echo htmlspecialchars(\"<!---\");?>/foreach<?php echo htmlspecialchars(\"--->\");?><br />\n"
"      Voorwaarden:<br />\n"
"      -Niet leeg:  <?php echo htmlspecialchars(\"<!---\");?>if !empty($<b>fieldname</b>)<?php echo htmlspecialchars(\"--->\");?><br />\n"
"     Einde voorwaarde blok:  <?php echo htmlspecialchars(\"<!---\");?>/if<?php echo htmlspecialchars(\"--->\");?><br />\n"
"      Afbeelding: [image src={$<b>fieldname</b>} w=<b>width</b> h=<b>height</b> border=<b>border</b> bcolor=<b>bordercolor</b> ]"

